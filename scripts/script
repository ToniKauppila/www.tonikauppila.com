var animation = bodymovin.loadAnimation({
    container: document.getElementById('lottie'), // the dom element that will contain the animation
    renderer: 'svg',
    loop: true,
    autoplay: true,
    path: 'kuitti_animaatio.json' // the path to the animation json
  });   

